    "incremental": true
  }, {
    "kind": "class",
    "fileName": " module.translateQualifiedName(module.packageName, 'groovy') + '.GroovyStaticExtension.java'",
    "templateFileName": "vertx-groovy/template/ext-static-methods.templ",
    "incremental": true
  }, {
    "kind": "module",
    "fileName": "'resources/META-INF/services/org.codehaus.groovy.runtime.ExtensionModule'",
    "templateFileName": "vertx-groovy/template/ext-desc.templ",
    "incremental": true
  } ]
}
